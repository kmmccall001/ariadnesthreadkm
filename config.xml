<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.converse.kmmccall001.ariadnesthread" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ariadne's thread</name>
